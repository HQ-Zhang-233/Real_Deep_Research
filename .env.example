# Gemini API配置（用于AI对话）
GEMINI_API_KEY=your_gemini_api_key_here

# Coze API配置（必需，用于获取YouTube视频字幕）
COZE_API_TOKEN=your_coze_api_token_here
COZE_WORKFLOW_ID=your_coze_workflow_id_here